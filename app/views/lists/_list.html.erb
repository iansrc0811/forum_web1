<% obj.each do |list| %>
<tr>
  <td>
    <%= link_to "#{list.name}", list_path(list)%>
  </td>
  <td>
    <%= simple_format(list.description) %>
  </td>
  <% if current_user %>
    <td>
      <%= link_to 'edit', edit_list_path(list) %>
    </td>    

    <td>
      <%= link_to 'delete', list_path(list), method: :delete %>
    </td> 
  <% end %>   
</tr>
<% end %>